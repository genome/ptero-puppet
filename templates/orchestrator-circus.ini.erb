[watcher:orchestrator]
cmd = <%= @circus_command %>
args = <%= @circus_args %>
numprocesses = <%= @circus_numprocesses %>
working_dir = <%= @circus_working_dir %>
uid = <%= @circus_uid %>
gid = <%= @circus_gid %>

stderr_stream.class = FileStream
stderr_stream.filename = <%= @code_dir %>/orchestrator.log
stderr_stream.max_bytes = 1048576
stderr_stream.backup_count = 1

<% if @circus_environment -%>
[env:orchestrator]
<% @circus_environment.each do |name, val| -%>
<%= name %> = <%= val %>
<% end -%>
<% end -%>
